手順新規登録
<%= form_with model: @procedure , local:true, url: admin_procedure_path %>
  <%= f.file_field :image %>
  <%= f.text_area :item %>
  <%= f.text_area :procedure %>
<% end %>

<p>変更件数：<%= @procedure.changes.count %></p>
<% @procedure.changes.each do |procedure| %>
  <%= change.user.name %>
  <%= change.created_at.strftime('%Y/%m/%d') %>
<% end %>

<%= form_with model: [@procedure, @change] do |f| %>
  <%= f.text_area :change, rows:'5', placeholder: "変更。追加点をここに" %>
  <%= f.submit "送信する" %>
<% end %>